PROGRAM STATS
VAR
	1SUM, SUMSQ, I, VALUE, MEAN, VARIANCE : INTEGER
BEGIN
	1SUM := 0;
	SUMSQ := 0;
	FOR I := 1 TO 100 DO
		BEGIN
			READ(VALUE);
			1SUM := 1SUM + VALUE;
			SUMSQ := SUMSQ + VALUE * VALUE
		END;
	MEAN := 1SUM DIV 100;
	VARIANCE := SUMSQ DIV 100 - MEAN * MEAN;
	WRITE(MEAN,VARIANCE)
END.
